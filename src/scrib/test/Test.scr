module scrib.test.Test;  // Use scrib prefix to separate from general go packages


type <go> "int" from "..." as int;
type <go> "string" from "..." as string;


global protocol Proto1(role A, role B)
{
	choice at A
	{
		M1(int) from A to B;
		M1(int) from B to A;
		do Proto1(A, B);
	}
	or
	{
		M2(string) from A to B;
		M2(string) from B to A;
	}
}


global protocol Proto2(role A, role B)
{
	choice at A
	{
		Ok() from A to B;
	}
	or
	{
		Bye() from A to B;
	}
	choice at A
	{
		Ok() from A to B;
	}
	or
	{
		Bye() from A to B;
	}
}


global protocol Proto3(role A, role B, role C)
{
	choice at A
	{
		Ok() from A to B;
	}
	or
	{
		Bye() from A to B;
	}
	choice at B
	{
		Ok() from B to A;
	}
	or
	{
		Bye() from B to A;
	}
	choice at B
	{
		Ok() from B to A;
		Ok() from B to C;
	}
	or
	{
		Bye() from B to A;
		Bye() from B to C;
	}
}
